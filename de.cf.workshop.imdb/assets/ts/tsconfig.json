{
    "extends": "../../../../main/tsconfig.base.json",
    "compilerOptions": {
        "rootDir": ".",
        "baseUrl": ".",
        "outDir": "../generated_js",
        "sourceMap": true,
        "declarationMap": true,
        "paths": {
            "@cf.cplace.platform/*": [
                "../../../../main/cf.cplace.platform/assets/ts/*"
